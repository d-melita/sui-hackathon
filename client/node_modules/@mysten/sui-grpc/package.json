{
  "name": "@mysten/sui-grpc",
  "version": "0.1.0",
  "description": "A typescript GRPC client for sui",
  "license": "Apache-2.0",
  "author": "Mysten Labs <build@mystenlabs.com>",
  "sideEffects": false,
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MystenLabs/ts-sdks.git"
  },
  "keywords": [
    "sui",
    "grpc"
  ],
  "bugs": {
    "url": "https://github.com/MystenLabs/ts-sdks/issues"
  },
  "homepage": "https://github.com/MystenLabs/ts-sdks/tree/main/packages/sui-grpc#readme",
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "@mysten/build-scripts": "0.0.0"
  },
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@protobuf-ts/runtime": "^2.11.1",
    "@protobuf-ts/runtime-rpc": "^2.11.1",
    "@mysten/sui": "1.38.0",
    "@mysten/utils": "0.2.0"
  },
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "dev": "pnpm build --watch",
    "build": "build-package",
    "prettier:check": "prettier -c --ignore-unknown .",
    "prettier:fix": "prettier -w --ignore-unknown .",
    "eslint:check": "eslint --max-warnings=0 .",
    "eslint:fix": "pnpm run eslint:check --fix",
    "lint": "pnpm run eslint:check && pnpm run prettier:check",
    "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
    "generate:proto": "protoc --ts_out ./src/proto --ts_opt server_none -I ../../../sui-apis/proto -I ../../../sui-apis/proto ../../../sui-apis/proto/sui/rpc/v2beta2/*.proto ../../../sui-apis/proto/google/*/*.proto",
    "generate": "rm -rf src/proto && mkdir src/proto && pnpm generate:proto && pnpm lint:fix"
  }
}